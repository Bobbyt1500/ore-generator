{
  "schemaVersion": 1,
  "id": "ore-generator",
  "version": "${version}",

  "name": "Ore Generator",
  "description": "Allows the creation of Ore Generations using the corresponding Ore Fluid",
  "authors": [
    "Bobby15",
    "Ovenjohnson",
    "Chance"
  ],
  "contact": {
    "sources": "https://github.com/Bobbyt1500/ore-generator"
  },

  "license": "CC0-1.0",
  "icon": "assets/ore-generator/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.oregenerator.OreGeneratorMod"
    ],
    "client": [
      "net.fabricmc.oregenerator.OreGeneratorModClient"
    ]
  },
  "mixins": [
    "ore-generator.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  }
}
